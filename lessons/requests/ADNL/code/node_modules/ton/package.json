    "jest-mock": "^27.1.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.5.2",
    "karma-webpack": "^5.0.0",
    "prando": "^6.0.1",
    "release-it": "^15.5.1",
    "ton-core": "^0.48.0",
    "ton-crypto": "3.2.0",
    "ton-emulator": "^2.1.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
